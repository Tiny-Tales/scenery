        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "examples",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "COMING SOON TM"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "tsup": "^8.0.1",
    "prettier": "3.1.1",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "pixi.js": "^7.3.2"
  }
}